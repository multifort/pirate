<project>
  <description />
  <keepDependencies>false</keepDependencies>
  <properties />
  <scm class="hudson.plugins.git.GitSCM" plugin="git@2.5.3">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>${repositoryUrl}</url>
        ${has(credentialId)?"<credentialsId>"+credentialId+"</credentialsId>":""}
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>${repositoryBranch}</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="list" />
    <extensions />
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers />
  <concurrentBuild>false</concurrentBuild>
  <builders />
  <publishers />
  <buildWrappers />
</project>
{"parameters":
  [
    {
	  "value": "源码git克隆组件，目前仅支持github和gitlab两种git仓库",
	  "type": "warn"
	},
    {
      "description": "git克隆的地址",
      "displayName": "git地址",
      "name": "repositoryUrl",
      "value": "",
      "select": "",
      "type": "Api",
      "interface":"code/repository/code",
      "response": "codeSource",
      "method": "GET",
      "params": {"softwareType":"0", "status":"0"},
      "required": "true",
      "event":"double_load"
    },
    {
      "description": "访问源码地址用户名，gitlab类型为必填项",
      "displayName": "源码用户名",
      "name": "username",
      "value": "",
      "select": "",
      "type": "String",
      "required": "false",
      "display":"false"
    },
    {
      "description": "访问源码地址密码，gitlab类型为必填项",
      "displayName": "源码密码",
      "name": "password",
      "value": "",
      "select": "",
      "type": "Password",
      "required": "false",
      "display":"false"
    },
    {
      "description": "jenkins凭证",
      "displayName": "jenkins凭证",
      "name": "credentialId",
      "value": "",
      "select": "",
      "type": "Api",
      "interface":"jenkinsCredential/queryCredentialId",
      "method": "GET",
      "parm":["username","password"],
      "mast":["username","password"],
      "event":"confirm",
      "required": "true",
      "display":"false"
    },
    {
      "description": "填写git分支",
      "displayName": "git分支",
      "name": "repositoryBranch",
      "value": "",
      "select": "master",
      "type": "String",
      "required": "true"
    },
  ]
}